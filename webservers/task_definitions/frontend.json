[
  {
    "cpu": 10,
    "essential": true,
    "image": "${account_number}.dkr.ecr.${region}.amazonaws.com/micropost/frontend:latest",
    "memory": 300,
    "name": "frontend",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${region}"
      }
    }
  }
]
